#ifndef STUDENT_H
#define STUDENT_H
#include <cstring>

class Student {
  private:
  char* first;
  char* last;
  int id;
  float gpa;
public:
  Student(char* first, char* last, int id, float gpa);
  ~Student();

  char* getFirst();
  char* getLast();
  int getID();
  float getGPA();

  void setFirst(char* newFirst);
  void setLast(char* newLast);
  void setID(int newID);
  void setGPA(float newGPA);
};

#endif
